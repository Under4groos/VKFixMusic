{
  "name": "Google Search Block",
  "version": "1.1",
  "description": "Удаляет лишние сайты",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "action": {
    "default_title": "VK Music Fix",
    "default_popup": "index.html"
  },
  "icons": {
    "256": "images/favicon.png"
  }
   
}
